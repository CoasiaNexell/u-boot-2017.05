#include "sip-s31nx.dtsi"

/ {
	compatible = "samsung,artik310s", "nexell,nxp3220";

	chosen {
		tick-timer = &timer0;
	};
};

&clock_pll {
	frequency = <0 800000000>,
		    <1 400000000>;
};

&clock_sys {
	clk-init-frequency = <TIMER_0_APB_CLK 100000000 1>,
			     <TIMER_0_TCLK0_CLK 100000000 1>,
			     <TIMER_0_TCLK1_CLK 100000000 1>;
};

&i2c_0 {
	status = "okay";
	u-boot,dm-pre-reloc;
	#address-cells = <1>;
	#size-cells = <0>;

	sm5011: sm5011@47 {
		compatible = "sm,sm5011";
		reg = <0x47>;

		voltage-regulators {
			/* BUCK */
			dcdc2: BUCK2 {
				regulator-name = "sm5011-buck2";
				regulator-min-microvolt = <562500>;
				regulator-max-microvolt = <2150000>;
				always_onoff_npwrstm = <0>;
				sm5011,init_uV = <1000000>;
				sm5011,on = <1>;
				regulator-always-on;
			};

			dcdc3: BUCK3 {
				regulator-name = "sm5011-buck3";
				regulator-min-microvolt = <562500>;
				regulator-max-microvolt = <2150000>;
				always_onoff_npwrstm = <0>;
				sm5011,init_uV = <1500000>;
				sm5011,on = <1>;
				regulator-always-on;
				regulator-boot-on;
			};

			dcdc4: BUCK4 {
				regulator-name = "sm5011-buck4";
				regulator-min-microvolt = <562500>;
				regulator-max-microvolt = <2150000>;
				always_onoff_npwrstm = <0>;
				sm5011,init_uV = <1000000>;
				sm5011,on = <1>;
				regulator-always-on;
				regulator-boot-on;
			};

			dcdc5: BUCK5 {
				regulator-name = "sm5011-buck5";
				regulator-min-microvolt = <562500>;
				regulator-max-microvolt = <2150000>;
				always_onoff_npwrstm = <0>;
				sm5011,init_uV = <1000000>;
				sm5011,on = <1>;
				regulator-always-on;
			};

			dcdc6: BUCK6 {
				regulator-name = "sm5011-buck6";
				regulator-min-microvolt = <562500>;
				regulator-max-microvolt = <2150000>;
				always_onoff_npwrstm = <0>;
				sm5011,init_uV = <1500000>;
				sm5011,on = <1>;
				regulator-always-on;
				regulator-boot-on;
			};

			/* LDO */
			ldo1_reg: LDO1 {
				regulator-name = "sm5011-ldo1";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
				sm5011,init_uV = <1000000>;
				sm5011,on = <1>;
