			};

			ldo2_reg: LDO2 {
				regulator-name = "sm5011-ldo2";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
				sm5011,init_uV = <1800000>;
				sm5011,on = <1>;
				regulator-always-on;
			};

			ldo3_reg: LDO3 {
				regulator-name = "sm5011-ldo3";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
				sm5011,init_uV = <3300000>;
				sm5011,on = <1>;
				regulator-always-on;
			};

			ldo4_reg: LDO4 {
