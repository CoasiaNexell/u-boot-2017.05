				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
				sm5011,init_uV = <1800000>;
				sm5011,on = <1>;
				regulator-always-on;
				regulator-boot-on;
			};

			ldo16_reg: LDO16 {
				regulator-name = "sm5011-ldo16";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
				sm5011,init_uV = <0000000>;
				sm5011,on = <0>;
			};

			ldo17_reg: LDO17 {
				regulator-name = "sm5011-ldo17";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
				sm5011,init_uV = <0000000>;
				sm5011,on = <0>;
			};

			ldo18_reg: LDO18 {
				regulator-name = "sm5011-ldo18";
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3350000>;
				always_onoff_npwrstm = <0>;
				always_lpm_npwrstm = <0>;
